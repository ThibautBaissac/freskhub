<div id="username_<%= dom_id(@user) %>" class="flex justify-between items-center mb-5">
  <h1><%= @user.firstname %></h1>
  <%= link_to(edit_user_path(@user), data: {turbo_frame: :modal, action: "dialog#open"}) do %>
    <%= render(Svg::EditComponent.new(stroke_width: 1.5, classes: "w-8 h-8 stroke-gray-700")) %>
  <% end %>
</div>
